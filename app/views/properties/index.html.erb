<h1>Western Suburb Agency </h1>
<h1><%= @message %></h1>
<form class="form-inline my-2 my-lg-0" action="<%= properties_path %>">
  <input class="form-control mr-sm-2" type="search" name="search" placeholder="Search by Postcode" aria-label="Search">

  <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
</form>

<div class="index">
  <% @properties.each do |property| %>
  <div class="propertycard">
    <p><%= property.adress%></p>
    <%= link_to cl_image_tag(property.photos, :class => 'thumb'), property %>
  </div>
  <% end %>
</div>
