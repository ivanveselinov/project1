<div class="formformat">


<%= form_for @agents do |f|  %>
<br>
<%= f.label :name, "Name: " %>
<%= f.text_field :name %>
</br>
<br>
<%= f.label :surname, "Surname: " %>
<%= f.text_field :surname %>
</br>
<br>
<%= f.label :mobile, "Mobile: " %>
<%= f.text_field :mobile %>
</br>
<br>
<%= f.label :phone, "Phone: " %>
<%= f.text_field :phone %>
</br>
<br>
<%= f.label :email, "Email: " %>
<%= f.text_field :email %>
</br>
<br>
<%= f.label :image, "Profile Photo: " %>
<%= f.url_field :image %>
</br>
<br>
<%= f.submit %>
</br>
<% end %>

</div>
