
<div class="formformat">
<%= form_for @properties do |f| %>
<br>
<%= f.label :adress, "Adress: " %>
<%= f.text_field :adress, :required => true %>
</br>
<br>
<%= f.label :suburb, "Suburb: "%>
<%= f.text_field :suburb, :required => true %>
</br>
<br>
<%= f.label :postcode, "PostCode: " %>
<%= f.text_field :postcode, :required => true %>
</br>
<br>
<%= f.label :rooms, "Rooms: " %>
<%= f.text_field :rooms %>
</br>
<br>
<%= f.label :livingrooms, "LivingRooms: " %>
<%= f.text_field :livingrooms %>
</br>
<br>
<%= f.label :bedrooms, "BedRooms: " %>
<%= f.text_field :bedrooms %>
</br>
<br>
<%= f.label :bathrooms, "BathRooms: " %>
<%= f.text_field :bathrooms %>
</br>
<br>
<%= f.label :toilets, "Toilets: " %>
<%= f.text_field :toilets %>
</br>
<br>
<%= f.label :garage, "Garage: " %>
<%= f.text_field :garage %>
</br>
<br>
<%= f.label :pspace, "Parking Space: " %>
<%= f.text_field :pspace %>
</br>
<br>
<%= f.label :rent, "Rent: " %>
<%= f.text_field :rent %>
</br>
<br>
<%= f.label :photos, "Main Photo: " %>
<%= f.url_field :photos, :required => true %>
</br>
<br>
<%= f.label :agent_ids, "Real Estate Agents: " %>
<%= collection_check_boxes :property, :agent_ids, Agent.all, :id, :name %>
</br>
<br>
<%= f.submit %>
</br>
<% end %>
</div>
